doctype html
html(lang='en')
	head
		link(href="main.css", rel="stylesheet")
		meta(charset="utf-8")
		title visualization 

	body
		audio(id="audioElement" src="/audio/Odesza-AboveTheMiddle.mp3")
		div
			button(onclick="document.getElementById('audioElement').play()") Play the Audio
			button(onclick="document.getElementById('audioElement').pause()") Pause the Audio
			button(onclick="document.getElementById('audioElement').volume+=0.1") Increase Volume
			button(onclick="document.getElementById('audioElement').volume-=0.1") Decrease Volume
		block content

		script(src="https://code.jquery.com/jquery-2.1.3.min.js")
		script(src="http://d3js.org/d3.v3.min.js" charset="utf-8")
		script(src="main.js")